
class Solution:
    def maximum69Number (self, num: int) -> int:
        l = list(str(num))
        k = ''
        
        for i in range(len(l)):
            if l[i] == '6':
                m = l[i]
                l[i] = '9'
                k = ''.join(l)
                return(int(k))
        return num
                
         
