class Solution:
    def transpose(self, A: List[List[int]]) -> List[List[int]]:
        rows=len(A)
        cols=len(A[0])
        transpose=[[0]*rows for i in range(cols)]
        
        for i in range(rows):
            for j in range(cols):
                transpose[j][i]=A[i][j]
                
                
        return transpose
        
